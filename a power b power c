#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {
    int a,b,c,x,y,n,i;
    scanf("%d",&n);
    for(int i=0;i<n;i++){
    scanf("%d",&a);
    scanf("%d",&b);
    scanf("%d",&c);
    
    x=pow(a,pow(b,c));
    if(x<9){
        y=pow(10,9);
        x=(x%y)+7;
        printf("%d\n",x);
    }
    else{
        printf("%d\n",x);
    }
    }
    return 0;
}
