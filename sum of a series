int sum_of_series(int n) {
        int i,j,sum=0;
        for(i=1;i<=n;i++){
            printf("%d ",i);
            sum= sum+i;
        }
        printf("\n");
        printf("%d ",sum);
        
        return n;
}

int main()

{
    int i,j,n;
    scanf("%d",&n);
    sum_of_series(n);
    return 0;
}

